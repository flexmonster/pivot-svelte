{
	"name": "pivot-svelte",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"start": "npm run open-browser & vite dev",
		"open-browser": "open-cli http://localhost:5173",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@amcharts/amcharts4": "^4.10.40",
		"@amcharts/amcharts5": "^5.12.2",
		"highcharts": "^12.2.0",
		"open-cli": "^8.0.0",
		"svelte-flexmonster": "latest"
	}
}
